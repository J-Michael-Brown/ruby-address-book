<form action="add_address" method="post">

  <label for="">Street Address</label>
  <input type="text" name="contact-street" placeholder="1234 Main st">

  <label for="">City</label>
  <input type="text" name="contact-city" placeholder="Portland">

  <label for="">Zip</label>
  <input type="text" name="contact-zip" placeholder="97201">

  <label for="">Contact Phone</label>
  <input type="text" name="contact-phone" placeholder="555-1234">


<button type="submit" name="create-address">Add Address</button>
</form>
